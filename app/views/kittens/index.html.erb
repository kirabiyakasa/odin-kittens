<section id='sub-container'>
  <ul>
  <%= link_to 'New Kitten', new_kitten_path %>
  <p>Kittens:</p>
    <% @kittens.each do |kitten| %>
      <li>
        <%= link_to kitten.name, kitten_path(kitten) %>
        <b>
          <%= link_to 'Delete', kitten_path(kitten), method: 'delete',
          class: "delete-button",
          data: { confirm: "Are you sure you want to delete this post?" } %>
        </b>
      </li>
    <% end %>
  </ul>
</section>
